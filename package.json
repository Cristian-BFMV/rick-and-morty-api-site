{
  "name": "rick-morty-api-site",
  "description": "Rick and Morty API site",
  "version": "1.0.0",
  "author": "Axel Fuhrmann",
  "dependencies": {
    "babel-plugin-styled-components": "^1.8.0",
    "dotenv": "^6.1.0",
    "gatsby": "^2.0.28",
