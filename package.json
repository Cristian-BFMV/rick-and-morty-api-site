    "typography-theme-github": "^0.15.10"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "cypress": "^3.0.3",
    "eslint": "^5.2.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "stylelint": "^9.4.0",
    "stylelint-config-recommended": "^2.1.0",
