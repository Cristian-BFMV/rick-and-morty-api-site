    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-remark-autolink-headers": "^2.0.7",
    "gatsby-remark-prismjs": "^3.0.2",
    "gatsby-source-filesystem": "^2.0.5",
    "gatsby-transformer-remark": "^2.1.8",
    "gatsby-transformer-yaml": "^2.1.3",
    "moment": "^2.22.2",
    "polished": "^2.3.0",
    "prismjs": "^1.15.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.2.1",
    "react-spinkit": "^3.0.0",
    "react-typography": "^0.16.13",
    "rickmortyapi": "^0.2.1",
    "styled-components": "^4.0.2",
    "typography": "^0.16.17",
    "typography-theme-github": "^0.15.10"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cypress": "^3.1.0",
    "eslint": "^5.7.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "npm-run-all": "^4.1.3",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "yamljs": "^0.3.0"
  },
  "keywords": [
    "gatsby",
    "api",
    "rick and morty"
  ],
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "cypress": "cypress run --browser chrome",
    "test": "run-p serve cypress",
    "deploy": "npm run build && node deploy.js",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:js": "eslint ./src/. --fix",
    "lint": "run-s lint:js lint:css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "stylelint",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afuh/rick-and-morty-api-site.git"
  },
  "bugs": {
    "url": "https://github.com/afuh/rick-and-morty-api-site/issues"
  },
  "homepage": "https://github.com/afuh/rick-and-morty-api-site#readme"
